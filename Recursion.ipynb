    "# Reverse an array \n",
    "\n",
    "arr=[1,2,3,4,5]\n",
    "\n",
    "def reverse_array(i,arr,n):\n",
    "    if i >=n/2:\n",
    "        return \n",
    "    arr[i],arr[n-i-1]= arr[n-i-1],arr[i]\n",
    "    reverse_array(i+1,arr,n)\n",
    "    \n",
    "reverse_array(0, arr,len(arr))\n",
    "print(arr)\n",
    "    "
   ]
